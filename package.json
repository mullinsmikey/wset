{
  "name": "watch_set",
  "version": "2.0.0",
  "author": "Blue Muffin",
  "homepage": "http://muffin.blue/",
  "description": "GPS watch config app",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ic:run-w-errors": "npm run ioco:a:run && npm run ioco:a:adb-log-tiny",
    "ic:run-w-errors-ssar": "npm run ioco:a:run-ssar && npm run ioco:a:adb-log-tiny",
    "ic:rerun-w-errors": "npm run ioco:a:run-ssar-nb && npm run ioco:a:adb-log-tiny",
    "ic:run-w-logs": "npm run ioco:a:run && npm run ioco:a:adb-log-full",
    "ic:run-live-design": "ionic cordova run android --device --livereload --debug",
    "ic:reload-android": "npm run ioco:a:rm-ns && rm -rf platforms/android && npm run ioco:a:add-ns",
    "ic:reload-android-f": "npm run ioco:a:rm-ns && rm -rf platforms/android && rm -rf plugins/* && npm run ioco:a:add-ns",
    "ioco:a:run": "ionic cordova run android --device",
    "ioco:a:run-nb": "ionic cordova run android --device --no-build",
    "ioco:a:run-ssar": "SENTRY_SKIP_AUTO_RELEASE=true ionic cordova run android --device",
    "ioco:a:run-ssar-nb": "SENTRY_SKIP_AUTO_RELEASE=true ionic cordova run android --device --no-build",
    "ioco:a:adb-log-tiny": "adb logcat -c && adb logcat | grep -E \"chromium\"",
    "ioco:a:adb-log-full": "adb logcat -c && adb logcat | grep -E \"chromium|SQL|sql|ionic|Cordova|SERVER\"",
    "ioco:a:rm": "ionic cordova platform rm android",
    "ioco:a:rm-ns": "ionic cordova platform rm android --nosave",
    "ioco:a:add": "ionic cordova platform add android@latest",
    "ioco:a:add-ns": "ionic cordova platform add android@latest --nosave",
    "ic:release-android": "npm run ioco:ra:build && npm run ioco:ra:move && npm run ioco:ra:align && npm run ioco:ra:sign && npm run ioco:ra:clb",
    "ioco:ra:build": "npm run ioco:ra:prepare && npm run ioco:ra:compile",
    "ioco:ra:prepare": "SENTRY_SKIP_AUTO_RELEASE=true ionic cordova prepare android --prod",
    "ioco:ra:compile": "SENTRY_SKIP_AUTO_RELEASE=true ionic cordova compile android --release",
    "ioco:ra:move": "mv platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk platforms",
    "ioco:ra:align": "~/Library/Android/sdk/build-tools/29.0.0/zipalign -v -p 4 platforms/app-release-unsigned.apk platforms/app-release-aligned.apk",
    "ioco:ra:sign": "~/Library/Android/sdk/build-tools/29.0.0/apksigner sign --ks ~/bluemuffin.jks --out platforms/app-release.apk platforms/app-release-aligned.apk",
    "ioco:ra:clb": "rm platforms/app-release-unsigned.apk && rm platforms/app-release-aligned.apk"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^8.2.6",
    "@angular/core": "^8.2.6",
    "@angular/forms": "^8.2.6",
    "@angular/platform-browser": "^8.2.6",
    "@angular/platform-browser-dynamic": "^8.2.6",
    "@angular/router": "^8.2.6",
    "@ionic-native/core": "^5.13.0",
    "@ionic-native/sms": "^5.14.0",
    "@ionic-native/splash-screen": "^5.13.0",
    "@ionic-native/status-bar": "^5.13.0",
    "@ionic/angular": "^4.9.0",
    "@ionic/storage": "^2.2.0",
    "cordova-android": "8.0.0",
    "cordova-plugin-androidx": "^1.0.2",
    "cordova-plugin-androidx-adapter": "^1.1.0",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic-webview": "^4.1.1",
    "cordova-plugin-splashscreen": "^5.0.3",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "cordova-sms-plugin": "^1.0.0",
    "cordova-sqlite-storage": "^3.3.0",
    "cordova-support-android-plugin": "1.0.1",
    "core-js": "^2.6.9",
    "es6-promise-plugin": "4.2.2",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.803.4",
    "@angular-devkit/build-angular": "^0.803.4",
    "@angular-devkit/core": "^8.3.4",
    "@angular-devkit/schematics": "^8.3.4",
    "@angular/cli": "^8.3.4",
    "@angular/compiler": "^8.2.6",
    "@angular/compiler-cli": "^8.2.6",
    "@angular/language-service": "^8.2.6",
    "@ionic/angular-toolkit": "^2.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.14.18",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~8.1.1",
    "tslint": "~5.16.0",
    "typescript": "~3.5.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-sqlite-storage": {},
      "cordova-plugin-androidx": {},
      "cordova-plugin-androidx-adapter": {},
      "cordova-sms-plugin": {}
    },
    "platforms": [
      "android"
    ]
  }
}